def RPorS():Â 
    Rock = Button(12)
    Paper = Button(23)
    Scissors = Button(17)
    PlayerInput = "None"
    print("Rock Paper or Scissors?")
    while(PlayerInput == "None"):
        if Rock.is_pressed:
            PlayerInput = "Rock"
        elif Paper.is_preessed:
            PlayerInput = "Paper"
        elif Scissors.is_pressed:
            PlayerInput = "Scissors"
        else:
            time.sleep(.05)
    return PlayerInput
